pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('build') {
            steps {
                echo 'building'
            }
        }
        
        
        
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        
        
        
        
      stage('release') {
            steps {
                echo 'Releasing'
            }
        }
        
        
        stage('deploy') {
            steps {
                echo 'Deploying'
            }
        }
        
        
       
        
    }
}
